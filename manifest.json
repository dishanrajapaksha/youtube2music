{
  "name": "YouTube to YouTube Music",
  "description": "Chrome extensions to open YouTube links in YouTube Music.",
  "homepage_url": "https://github.com/kapoorlakshya/youtube2music",
  "version": "0.1",
  "author": "Lakshya Kapoor",
  "manifest_version": 2,
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "icons": {
      "19": "images/icon19.png",
      "38": "images/icon38.png"
    },
    "default_title": "YouTube to YouTube Music"
  },
  "icons": {
    "16": "images/icon16.png",
    "24": "images/icon24.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}